{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/products/Deck-Bells",
      "status": 301,
      "headers": { "Location": "/products" }
    },
    {
      "src": "/products/deck-bells",
      "status": 301,
      "headers": { "Location": "/products" }
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml",
      "headers": { 
        "Cache-Control": "public, max-age=3600, s-maxage=86400", 
        "Content-Type": "application/xml; charset=utf-8" 
      }
    },
    {
      "src": "/sitemap-static.xml",
      "dest": "/sitemap-static.xml",
      "headers": { 
        "Cache-Control": "public, max-age=3600, s-maxage=86400", 
        "Content-Type": "application/xml; charset=utf-8" 
      }
    },
    {
      "src": "/sitemap-collections.xml",
      "dest": "/sitemap-collections.xml",
      "headers": { 
        "Cache-Control": "public, max-age=3600, s-maxage=86400", 
        "Content-Type": "application/xml; charset=utf-8" 
      }
    },
    {
      "src": "/sitemap-categories.xml",
      "dest": "/sitemap-categories.xml",
      "headers": { 
        "Cache-Control": "public, max-age=3600, s-maxage=86400", 
        "Content-Type": "application/xml; charset=utf-8" 
      }
    },
    {
      "src": "/sitemap-products.xml",
      "dest": "/sitemap-products.xml",
      "headers": { 
        "Cache-Control": "public, max-age=3600, s-maxage=86400", 
        "Content-Type": "application/xml; charset=utf-8" 
      }
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt",
      "headers": { 
        "Cache-Control": "public, max-age=3600, s-maxage=86400", 
        "Content-Type": "text/plain; charset=utf-8" 
      }
    },
    {
      "src": "/site.webmanifest",
      "dest": "/site.webmanifest",
      "headers": { 
        "Cache-Control": "public, max-age=86400", 
        "Content-Type": "application/manifest+json; charset=utf-8" 
      }
    },
    {
      "src": "/assets/(.*)",
      "headers": { 
        "Cache-Control": "public, max-age=31536000, immutable" 
      }
    },
    {
      "src": "/service-worker.js",
      "headers": { "Cache-Control": "no-cache, no-store, must-revalidate" }
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/logo.png",
      "dest": "/logo.png",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/og-image.jpg",
      "dest": "/og-image.jpg",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/hero-new.jpg",
      "dest": "/hero-new.jpg",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/ES_Icon.svg",
      "dest": "/ES_Icon.svg",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/favicon-96x96.png",
      "dest": "/favicon-96x96.png",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/apple-touch-icon.png",
      "dest": "/apple-touch-icon.png",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/web-app-manifest-192x192.png",
      "dest": "/web-app-manifest-192x192.png",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/web-app-manifest-512x512.png",
      "dest": "/web-app-manifest-512x512.png",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/.*\\.(png|jpg|jpeg|gif|svg|ico|webp)$",
      "headers": { "Cache-Control": "public, max-age=604800" }
    },
    {
      "src": "/.*\\.(woff|woff2|ttf|otf|eot)$",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(self), interest-cohort=()"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}