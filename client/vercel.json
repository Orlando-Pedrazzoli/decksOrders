{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/products/Deck-Bells",
      "status": 301,
      "headers": { "Location": "/products" }
    },
    {
      "src": "/products/deck-bells",
      "status": 301,
      "headers": { "Location": "/products" }
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml",
      "headers": { "Cache-Control": "public, max-age=3600", "Content-Type": "application/xml" }
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt",
      "headers": { "Cache-Control": "public, max-age=3600", "Content-Type": "text/plain" }
    },
    {
      "src": "/assets/(.*)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/service-worker.js",
      "headers": { "Cache-Control": "no-cache" }
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico",
      "headers": { "Cache-Control": "public, max-age=86400" }
    },
    {
      "src": "/logo.png",
      "dest": "/logo.png",
      "headers": { "Cache-Control": "public, max-age=86400" }
    },
    {
      "src": "/ES_Icon.svg",
      "dest": "/ES_Icon.svg",
      "headers": { "Cache-Control": "public, max-age=86400" }
    },
    {
      "src": "/favicon-32x32.png",
      "dest": "/favicon-32x32.png",
      "headers": { "Cache-Control": "public, max-age=86400" }
    },
    {
      "src": "/favicon-16x16.png",
      "dest": "/favicon-16x16.png",
      "headers": { "Cache-Control": "public, max-age=86400" }
    },
    {
      "src": "/apple-touch-icon.png",
      "dest": "/apple-touch-icon.png",
      "headers": { "Cache-Control": "public, max-age=86400" }
    },
    {
      "src": "/.*\\.(png|jpg|jpeg|gif|svg|ico|webp)$",
      "headers": { "Cache-Control": "public, max-age=86400" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}